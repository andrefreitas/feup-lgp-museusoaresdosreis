<% content_for :title, "Adicionar Administrador" %>
<%= render :partial => "admin/header" %>
<div class="box">
  <%= render :partial => "admin/menu"  %>
  <div class="actions">
    <%= render :partial => "admin/adminActions"  %>
  </div>
  <div class="view" id="listAdmins">

    <% @administrator.each do |admin|%>
        <div class="admin" id="<%=admin.id %>">
          <div class="name"><%=admin.name %></div>
          <div class="action"><a href="<%= edit_administrator_path(admin.id)%>"><button>Ver</button></a></div>
        </div>
    <% end %>

  </div>
</div>